{
  "name": "@ask-jenny/utils",
  "version": "1.0.0",
  "type": "module",
  "description": "Shared utility functions for Ask Jenny",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "default": "./dist/logger.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "ask-jenny",
    "utils"
  ],
  "author": "Ask Jenny Team",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "dependencies": {
    "@ask-jenny/platform": "1.0.0",
    "@ask-jenny/types": "1.0.0"
  },
  "devDependencies": {
    "@types/node": "22.19.3",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
