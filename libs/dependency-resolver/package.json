{
  "name": "@ask-jenny/dependency-resolver",
  "version": "1.0.0",
  "description": "Feature dependency resolution for Ask Jenny",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "ask-jenny",
    "dependency",
    "resolver"
  ],
  "author": "Ask Jenny Team",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "dependencies": {
    "@ask-jenny/types": "1.0.0"
  },
  "devDependencies": {
    "@types/node": "22.19.3",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
